<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Royalty</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/FactionDef[defName = "Empire"]</xpath>
					<value>
					<li Class="VanillaTradingExpanded.BankExtension">
						<bankNameKey>VTE.EmpireBankName</bankNameKey>
						<feesByGoodwill>
							<points>
								<li>(-100, 0.4)</li>
								<li>(-50, 0.3)</li>
								<li>(0, 0.2)</li>
								<li>(50, 0.1)</li>
								<li>(100, 0)</li>
							</points>
						</feesByGoodwill>
						<loanOptions>
							<li>
								<loanNameKey>VTE.QuickCash</loanNameKey>
								<fixedLoanAmount>10000</fixedLoanAmount>
								<fixedRepayAmount>15000</fixedRepayAmount>
								<transactionFeesIncluded>true</transactionFeesIncluded>
								<loanRepayPeriodDays>15</loanRepayPeriodDays>
							</li>
						</loanOptions>
					</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>